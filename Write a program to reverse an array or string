#include <iostream>
using namespace std;

int main() {
  int size; cin>>size;

  int array[size];

  //taking array inputs
  for(int i=0; i<size; i++){
    cin >> array[i];
  }

  //reverse the array
  for(int i=0; i<=size/2; i++){
    swap(array[i],array[size-1-i]);

  }
  
  //printing array
  for(int i=0; i<size; i++){
    cout<<array[i]<<" ";
  }

  return 0;
}
